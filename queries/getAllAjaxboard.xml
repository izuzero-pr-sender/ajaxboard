<query id="getAllAjaxboard" action="select">
	<tables>
		<table name="modules" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="module" default="ajaxboard" />
	</conditions>
	<navigation>
		<index var="sort_index" default="module_srl" order="asc" />
	</navigation>
</query>